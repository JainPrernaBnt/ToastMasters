<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/member_nav_graph"
    app:startDestination="@id/navigation_dashboard">

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.bntsoft.toastmasters.presentation.ui.members.dashboard.MemberDashboardFragment"
        android:label="Dashboard"
        tools:layout="@layout/fragment_member_dashboard">

        <action
            android:id="@+id/action_memberDashboardFragment_to_upcomingMeetingsFragment"
            app:destination="@id/navigation_meetings" />
        <action
            android:id="@+id/action_memberDashboardFragment_to_settingsFragment2"
            app:destination="@id/navigation_settings" />
        <action
            android:id="@+id/action_memberDashboardFragment_to_leaderboardFragment2"
            app:destination="@id/navigation_leaderboard" />
        <action
            android:id="@+id/action_navigation_dashboard_to_meetings_role"
            app:destination="@id/meetings_role" />
        <action
            android:id="@+id/action_memberDashboardFragment_to_agendaTableFragment"
            app:destination="@id/agendaTableFragment">
            <argument
                android:name="meetingId"
                android:defaultValue=""
                app:argType="string" />
        </action>
        <action
            android:id="@+id/action_memberDashboardFragment_to_createAgendaFragment"
            app:destination="@id/createAgendaFragment" />
    </fragment>

    <fragment
        android:id="@+id/navigation_meetings"
        android:name="com.bntsoft.toastmasters.presentation.ui.members.upcoming.UpcomingMeetingsFragment"
        android:label="Upcoming Meetings"
        tools:layout="@layout/fragment_upcoming_meetings" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.bntsoft.toastmasters.presentation.ui.common.settings.SettingsFragment"
        android:label="Settings"
        tools:layout="@layout/fragment_settings">
        <action
            android:id="@+id/action_settingsFragment2_to_memberDashboardFragment"
            app:destination="@id/navigation_dashboard" />
        <action
            android:id="@+id/action_settingsFragment_to_profileEditFragment"
            app:destination="@id/profileEditFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_clubMembersViewOnlyFragment"
            app:destination="@id/clubMembersViewOnlyFragment" />
    </fragment>
    <fragment
        android:id="@+id/navigation_leaderboard"
        android:name="com.bntsoft.toastmasters.presentation.ui.common.leaderboard.LeaderboardFragment"
        android:label="Leaderboard"
        tools:layout="@layout/fragment_leaderboard">
        <action
            android:id="@+id/action_leaderboardFragment2_to_memberDashboardFragment"
            app:destination="@id/navigation_dashboard" />
        <action
            android:id="@+id/action_leaderboardFragment_to_recentMeetingDetailFragment"
            app:destination="@id/recentMeetingDetailFragment" />
        <action
            android:id="@+id/action_leaderboardFragment_to_pastMeetingsFragment"
            app:destination="@id/pastMeetingsFragment" />
        <action
            android:id="@+id/action_leaderboardFragment_to_gemOfMonthFragment"
            app:destination="@id/gemOfMonthFragment" />
        <action
            android:id="@+id/action_leaderboardFragment_to_externalClubActivityFragment"
            app:destination="@+id/externalClubActivityFragment"/>
    </fragment>

    <fragment
        android:id="@+id/agendaTableFragment"
        android:name="com.bntsoft.toastmasters.presentation.ui.vp.agenda.AgendaTableFragment"
        android:label="Agenda"
        tools:layout="@layout/fragment_agenda_table">
        <argument
            android:name="meetingId"
            android:defaultValue=""
            app:argType="string" />
        <action
            android:id="@+id/action_agendaTableFragment_to_navigation_dashboard"
            app:destination="@id/navigation_dashboard" />
    </fragment>

    <fragment
        android:id="@+id/createAgendaFragment"
        android:name="com.bntsoft.toastmasters.presentation.ui.vp.agenda.front.CreateAgendaFragment"
        android:label="Create Agenda"
        tools:layout="@layout/fragment_create_agenda">
        <argument
            android:name="meeting_id"
            app:argType="string" />
        <action
            android:id="@+id/action_createAgendaFragment_to_agendaTableFragment"
            app:destination="@id/agendaTableFragment"
            app:popUpTo="@id/createAgendaFragment"
            app:popUpToInclusive="false">
            <argument
                android:name="meetingId"
                android:defaultValue="@null"
                app:argType="string"
                app:nullable="true" />
        </action>
    </fragment>

    <fragment
        android:id="@+id/meetings_role"
        android:name="com.bntsoft.toastmasters.presentation.ui.members.dashboard.MeetingsRoleFragment"
        android:label="Meetings Role"
        tools:layout="@layout/fragment_meetings_role">
        <action
            android:id="@+id/action_meetings_role_to_navigation_dashboard"
            app:destination="@id/navigation_dashboard" />
    </fragment>

    <fragment
        android:id="@+id/pastMeetingsFragment"
        android:name="com.bntsoft.toastmasters.presentation.ui.common.leaderboard.PastMeetingsFragment"
        android:label="PastMeetingsFragment"
        tools:layout="@layout/fragment_past_meetings">
        <action
            android:id="@+id/action_pastMeetingsFragment_to_leaderboardFragment"
            app:destination="@id/leaderboardFragment" />
    </fragment>
    <fragment
        android:id="@+id/recentMeetingDetailFragment"
        android:name="com.bntsoft.toastmasters.presentation.ui.common.leaderboard.RecentMeetingDetailFragment"
        android:label="RecentMeetingDetailFragment"
        tools:layout="@layout/fragment_recent_meeting_detail">
        <action
            android:id="@+id/action_recentMeetingDetailFragment_to_leaderboardFragment"
            app:destination="@id/leaderboardFragment" />
    </fragment>
    <fragment
        android:id="@+id/gemOfMonthFragment"
        android:name="com.bntsoft.toastmasters.presentation.ui.common.leaderboard.gem.GemOfMonthFragment"
        android:label="Gem of the Month"
        tools:layout="@layout/fragment_gem_of_month">
        <action
            android:id="@+id/action_gemOfMonthFragment_to_leaderboardFragment"
            app:destination="@id/leaderboardFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileEditFragment"
        android:name="com.bntsoft.toastmasters.presentation.ui.common.settings.ProfileEditFragment"
        android:label="Edit Profile"
        tools:layout="@layout/fragment_profile_edit" />
    <fragment
        android:id="@+id/addExternalActivityFragment"
        android:name="com.bntsoft.toastmasters.presentation.ui.common.leaderboard.external.AddExternalActivityFragment"
        android:label="AddExternalActivityFragment"
        tools:layout="@layout/fragment_add_external_activity">
        <action
            android:id="@+id/action_addExternalActivityFragment_to_externalClubActivityFragment"
            app:destination="@id/externalClubActivityFragment" />
    </fragment>
    <fragment
        android:id="@+id/externalClubActivityFragment"
        android:name="com.bntsoft.toastmasters.presentation.ui.common.leaderboard.external.ExternalClubActivityFragment"
        android:label="ExternalClubActivityFragment"
        tools:layout="@layout/fragment_external_club_activity">
        <action
            android:id="@+id/action_externalClubActivityFragment_to_addExternalActivityFragment"
            app:destination="@id/addExternalActivityFragment" />
        <action
            android:id="@+id/action_externalClubActivityFragment_to_leaderboardFragment"
            app:destination="@id/leaderboardFragment" />
    </fragment>
    <fragment
        android:id="@+id/clubMembersViewOnlyFragment"
        android:name="com.bntsoft.toastmasters.presentation.ui.common.clubmembers.ClubMembersViewOnlyFragment"
        android:label="Club Members"
        tools:layout="@layout/fragment_club_members_view_only" />
</navigation>
